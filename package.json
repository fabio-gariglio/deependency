    "di",
    "solid",
    "oop"
  ],
  "author": "Fabio Gariglio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fabio-gariglio/deependency/issues"
  },
  "homepage": "https://github.com/fabio-gariglio/deependency#readme",
  "devDependencies": {
    "should": "^8.3.1",
    "sinon": "^1.17.4"
  },
  "jshintConfig": {
    "esversion": 6,
    "globals": {
      "__dirname": true,
